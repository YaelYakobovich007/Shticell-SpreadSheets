    </servlet>

    <servlet-mapping>
        <servlet-name>IsVersionUpToDateServlet</servlet-name>
        <url-pattern>/isVersionUpToDate</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetSheetVersionCountServlet</servlet-name>
        <servlet-class>servlets.GetSheetVersionCountServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetSheetVersionCountServlet</servlet-name>
        <url-pattern>/getSheetVersionCount</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ClearSessionServlet</servlet-name>
        <servlet-class>servlets.ClearSessionServlet</servlet-class>
