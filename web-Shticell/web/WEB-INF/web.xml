        <servlet-class>servlets.SheetListServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SheetListServlet</servlet-name>
        <url-pattern>/sheetList</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SortRowsServlet</servlet-name>
        <servlet-class>servlets.SortRowsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SortRowsServlet</servlet-name>
        <url-pattern>/sortRows</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ValidateBoundariesServlet</servlet-name>
        <servlet-class>servlets.ValidateBoundariesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ValidateBoundariesServlet</servlet-name>
        <url-pattern>/validateBoundaries</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>IsCellOriginalValueNumericServlet</servlet-name>
        <servlet-class>servlets.IsCellOriginalValueNumericServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IsCellOriginalValueNumericServlet</servlet-name>
        <url-pattern>/isCellNumeric</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DynamicAnalysisServlet</servlet-name>
        <servlet-class>servlets.DynamicAnalysisServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DynamicAnalysisServlet</servlet-name>
        <url-pattern>/dynamicAnalysis</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetSheetByVersionServlet</servlet-name>
        <servlet-class>servlets.GetSheetByVersionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetSheetByVersionServlet</servlet-name>
        <url-pattern>/getSheetByVersion</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>PermissionListServlet</servlet-name>
        <servlet-class>servlets.PermissionListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PermissionListServlet</servlet-name>
        <url-pattern>/permissionList</url-pattern>
    </servlet-mapping>

    <servlet>
